<div>
  <div>
    <h1>
      List of your likes
    </h1>
  </div>
</div>


<hr>
<div>
  <div>
    <table border="1">
      <tr>

        <th>
          Creator
        </th>
        <th>
          Skill
        </th>

        <th>
          Link
        </th>
      </tr>

      <% @list_of_likes.each do |a_like| %>
      <tr>
        <td>
          <%= a_like.interest.user.username %>
        </td>

        <td>
          <%= a_like.interest.skill_name %>
        </td>

        <td>
        <a href = "/skills/<%=a_like.interest.id%>">
        Show Details
        </a>

      </tr>
      <% end %>
    </table>
  </div>
</div>

<hr>
